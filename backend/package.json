{
  "name": "kyndo-backend",
  "version": "1.0.0",
  "description": "KYNDO Backend API - Card management and thumbnail regeneration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test:smoke": "ts-node tests/smoke.test.ts",
    "start": "node dist/index.js",
    "worker": "ts-node src/worker/thumbnailWorker.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "bulk:promote": "ts-node scripts/bulk_promote.ts",
    "seed": "ts-node seeds/seed_dev.ts",
    "seed:birds": "ts-node seeds/seed_birds.ts",
    "backfill": "ts-node scripts/backfill_rarity_v2.ts",
    "export:json": "ts-node scripts/export_sqlite_to_json.ts"
  },
  "keywords": [
    "cards",
    "game",
    "api",
    "cognitive"
  ],
  "author": "KYNDO Team",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^129.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "prisma": "^5.8.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}

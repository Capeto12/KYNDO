    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.0"
  },
  "devDependencies": {
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.0"
  }
}
